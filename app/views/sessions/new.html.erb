<%= form_with scope: :session, url: {action: :create}, local: true do |f| %>
   <div class="form-group">
       <%= f.label :email %>
       <%= f.text_field :email, id: 'session_email' %>
   </div>
   <div class="form-group">
       <%= f.label :password %>
       <%= f.password_field :password, id: 'session_password' %>
   </div>
   <%= f.submit %>
<% end %>
